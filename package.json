{
  "name": "grue",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://grue.herokuapp.com/",
  "proxy": "http://localhost:1337/",
  "scripts": {
    "client": "yarn start",
    "server": "nodemon server",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "heroku-postbuild": "yarn && yarn run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chai": "^4.2.0",
    "express-graphql": "^0.6.12",
    "graphql": "0.13.0",
    "pg": "^7.4.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.5",
    "react-scroll-to-bottom": "^1.1.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.38.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.0.1",
    "eslint": "^5.6.0",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.2"
  }
}
